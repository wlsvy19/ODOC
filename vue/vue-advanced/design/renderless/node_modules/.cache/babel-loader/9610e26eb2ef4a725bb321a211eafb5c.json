{"ast":null,"code":"import axios from 'axios';\nimport FetchData from \"./components/FetchData.vue\";\nexport default {\n  components: {\n    FetchData\n  },\n  created() {\n    axios.get(this.url).then(response => {\n      this.response = response.data;\n      this.loading = false;\n    }).catch(error => {\n      alert('[ERROR] fetching the data', error);\n      console.log(error);\n    });\n  }\n};","map":{"version":3,"mappings":"AAiBA;AACA;AAEA;EACAA;IACAC;EACA;EACAC;IACAC,oBACAC;MACA;MACA;IACA,GACAC;MACAC;MACAC;IACA;EACA;AACA","names":["components","FetchData","created","axios","then","catch","alert","console"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div>\n    <fetch-data url=\"https://jsonplaceholder.typicode.com/users/1\">\n      <!-- 로직 -->\n      <div slot-scope=\"{ response, loading }\">\n        <div v-if=\"!loading\">\n          {{ response }}\n        </div>\n        <div v-if=\"loading\">\n          로딩중...\n        </div>\n      </div>\n    </fetch-data>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport FetchData from \"./components/FetchData.vue\";\n\nexport default {\n  components: {\n    FetchData,\n  },\n  created() {\n    axios.get(this.url)\n      .then(response => {\n        this.response = response.data;\n        this.loading = false;\n      })\n      .catch(error => {\n        alert('[ERROR] fetching the data', error);\n        console.log(error);\n      });\n  }\n};\n</script>"]},"metadata":{},"sourceType":"module"}