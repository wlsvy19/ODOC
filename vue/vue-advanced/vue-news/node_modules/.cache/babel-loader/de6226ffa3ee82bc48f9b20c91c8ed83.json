{"ast":null,"code":"export default {\n  created() {\n    const name = this.$route.name;\n    if (name === \"news\") {\n      this.$store.dispatch(\"FETCH_NEWS\");\n    } else if (name === \"ask\") {\n      this.$store.dispatch(\"FETCH_ASK\");\n    } else if (name === \"jobs\") {\n      this.$store.dispatch(\"FETCH_JOBS\");\n    }\n  },\n  computed: {\n    listItems() {\n      const name = this.$route.name;\n      if (name === \"news\") {\n        return this.$store.state.news;\n      } else if (name === \"ask\") {\n        return this.$store.state.ask;\n      } else if (name === \"jobs\") {\n        return this.$store.state.jobs;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAqCA;EACAA;IACA;IAEA;MACA;IACA;MACA;IACA;MACA;IACA;EACA;EACAC;IACAC;MACA;MACA;QACA;MACA;QACA;MACA;QACA;MACA;IACA;EACA;AACA","names":["created","computed","listItems"],"sourceRoot":"src/components","sources":["ListItem.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <ul class=\"news-list\">\r\n      <li v-for=\"item in listItems\" class=\"post\">\r\n        <!-- 포인트 영역(숫자나오는 영역)-->\r\n        <div class=\"points\">\r\n          {{ item.points || 0 }}\r\n        </div>\r\n        <!-- 기타 정보 영역: 유저 내용들-->\r\n        <div>\r\n          <!--  타이틀 영역 -->\r\n          <p class=\"news-title\">\r\n            <template v-if=\"\">\r\n              <a v-bind:href=\"item.url\">\r\n                {{ item.title }}\r\n              </a>\r\n            </template>\r\n            <template v-else>\r\n              <router-link v-bind:to=\"`item/${item.id}`\">\r\n                {{item.title}}\r\n              </router-link>\r\n            </template>\r\n\r\n          </p>\r\n          <small class=\"link-text\">\r\n            {{ item.time_ago }} by\r\n            <router-link v-bind:to=\"`/user/${item.user}`\" class=\"link-text\">{{\r\n              item.user\r\n            }}</router-link>\r\n          </small>\r\n        </div>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  created() {\r\n    const name = this.$route.name;\r\n\r\n    if (name === \"news\") {\r\n      this.$store.dispatch(\"FETCH_NEWS\");\r\n    } else if (name === \"ask\") {\r\n      this.$store.dispatch(\"FETCH_ASK\");\r\n    } else if (name === \"jobs\") {\r\n      this.$store.dispatch(\"FETCH_JOBS\");\r\n    }\r\n  },\r\n  computed: {\r\n    listItems() {\r\n      const name = this.$route.name;\r\n      if (name === \"news\") {\r\n        return this.$store.state.news;\r\n      } else if (name === \"ask\") {\r\n        return this.$store.state.ask;\r\n      } else if (name === \"jobs\") {\r\n        return this.$store.state.jobs;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.news-list {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.post {\r\n  list-style: none;\r\n  display: flex;\r\n  align-items: center;\r\n  border-bottom: 1px solid #eee;\r\n}\r\n.points {\r\n  width: 80px;\r\n  height: 60px;\r\n  display: flex;\r\n  /* 수평 정렬 */\r\n  align-items: center;\r\n  /* 수직 정렬 */\r\n  justify-content: center;\r\n  /* vue logo hex color */\r\n  color: #42b883;\r\n}\r\n.news-title {\r\n  margin: 0;\r\n}\r\n.link-text {\r\n  color: #828282;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}